{
    "seed": 64,
    
    "dataset": "pacs",
    "test_domain": ["photo"], 
    
    "local_json_dir_name": "tmp",
    
    "learning_rate": 1e-4,
    "decay_every": 5000,
    "decay_base": 0.96,
    "batch_size": 16,
    "num_epochs": 20,
    
    "l2_penalty_weight": 1e-3,
    
    "eval_every_n_epochs": 5,
    "save_summary_steps": 500,
    "save_checkpoints_secs": 600,
    "log_step_count_steps": 500,

    "num_classes": 7,
    "name": "ResNet50",
    "resnet_weights": "imagenet",

    "dropout_rate": 0.5, 
    "use_dropout": 1
}